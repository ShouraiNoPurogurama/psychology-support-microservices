{
  "ReverseProxy": {
    "RouteGroups": {
      "profile-group": {
        "ClusterId": "profile-cluster",
        "RateLimiterPolicy": "fixed",
        "Routes": {
          "profile-health": {
            "Match": { "Path": "/profile-service/health" },
            "Transforms": [ { "PathPattern": "/health" } ]
          },
          "profile-swagger": {
            "Match": { "Path": "/profile-service/swagger/{**catch-all}" },
            "Transforms": [ { "PathPattern": "/swagger/{**catch-all}" } ]
          },
          "profile-route": {
            "Match": { "Path": "/profile-service/{**catch-all}" },
            "Transforms": [ { "PathPattern": "/{**catch-all}" } ]
          }
        }
      }
    },
    "Clusters": {
      "profile-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:5501/"
          }
        }
      }
    }
  }
}

