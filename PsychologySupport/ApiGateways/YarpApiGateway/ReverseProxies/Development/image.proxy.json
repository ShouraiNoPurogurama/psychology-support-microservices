{
  "ReverseProxy": {
    "RouteGroups": {
      "image-group": {
        "ClusterId": "image-cluster",
        "RateLimiterPolicy": "fixed",
        "Routes": {
          "image-health": {
            "Match": { "Path": "/image-service/health" },
            "Transforms": [ { "PathPattern": "/health" } ]
          },
          "image-swagger": {
            "Match": { "Path": "/image-service/swagger/{**catch-all}" },
            "Transforms": [ { "PathPattern": "/swagger/{**catch-all}" } ]
          },
          "image-route": {
            "Match": { "Path": "/image-service/{**catch-all}" },
            "Transforms": [ { "PathPattern": "/{**catch-all}" } ]
          }
        }
      }
    },
    "Clusters": {
      "image-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:5511/"
          }
        }
      }
    }
  }
}

