{
  "ReverseProxy": {
    "Routes": {
      "scheduling-health": {
        "ClusterId": "scheduling-cluster",
        "Match": {
          "Path": "/scheduling-service/health"
        },
        "Transforms": [ { "PathPattern": "/health" } ]
      },
      "scheduling-swagger": {
        "ClusterId": "scheduling-cluster",
        "Match": {
          "Path": "/scheduling-service/swagger/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "/swagger/{**catch-all}" } ]
      },
      "scheduling-route": {
        "ClusterId": "scheduling-cluster",
        "RateLimiterPolicy": "fixed",
        "Match": {
          "Path": "/scheduling-service/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "/{**catch-all}" } ]
      }
    },
    "Clusters": {
      "scheduling-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:5502/"
          }
        }
      }
    }
  }
}
