{
  "ReverseProxy": {
    "Routes": {
      "feed-health": {
        "ClusterId": "feed-cluster",
        "Match": {
          "Path": "/feed-service/health"
        },
        "Transforms": [ { "PathPattern": "/health" } ]
      },
      "feed-swagger": {
        "ClusterId": "feed-cluster",
        "Match": {
          "Path": "/feed-service/swagger/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "/swagger/{**catch-all}" } ]
      },
      "feed-route": {
        "ClusterId": "feed-cluster",
        "RateLimiterPolicy": "fixed",
        "Match": {
          "Path": "/feed-service/{**catch-all}"
        },
        "Transforms": [ 
          { "PathPattern": "/{**catch-all}" },
          { "TokenExchange": "true" } ]
      }
    },
    "Clusters": {
      "feed-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://feed.api:8080/"
          }
        }
      }
    }
  }
}
